{
  "name": "tbTest",
  "version": "1.0.0",
  "description": "tester during development for Jove",
  "backMain": "src/tbAppBack.ts",
  "frontMain": "src/tbAppFront.ts",
  "dependencies": {
    "@riotjs/webpack-loader": "^5.0.0",
    "awesome-typescript-loader": "^5.2.1",
    "css-element-queries": "^1.2.3",
    "riot": "^5.3.3",
    "sourcemap-codec": "^1.4.8",
    "typescript": "^4.2.4"
  },
  "devDependencies": {
    "electron-builder": "^22.11.7"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "devMake": "./make",
    "devRun": "./make && tbx run",
    "build": "tbx build",
    "run": "tbx run",
    "nativescript": "./make && tbx nativescript",
    "nsAlone": "tbx nativescript",
    "makeMobile": "./makemobile",
    "dist": "tbx build --clean && electron-builder -mw"
  },
  "author": "tremho",
  "license": "ISC",
  "build": {
    "appId": "com.tremho.tbtest",
    "copyright": "Â©2021 Tremho Berserker Development, LLC",
    "electronVersion": "12.0.5",
    "mac": {
      "category": "public.app-category.utilities",
      "target": "dmg",
      "type": "development"
    },
    "directories": {
      "output": "dist",
      "buildResources": "build"
    },
    "asar": true,
    "files": [
      {
        "filter": "tbAppBack.js",
        "from": "build",
        "to": "."
      },
      {
        "filter": "package.json",
        "from": ".",
        "to": "."
      },
      {
        "filter": "**/*",
        "from": "build",
        "to": "."
      },
      {
        "filter": "**/*",
        "from": "node_modules",
        "to": "node_modules"
      }
    ],
    "extraMetadata": {
      "main": "tbAppBack.js"
    }
  }
}
